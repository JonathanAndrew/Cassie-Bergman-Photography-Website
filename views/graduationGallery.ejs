<div class="container">
	<a class="btn back-btn btn-lg btn-primary glyphicon glyphicon-arrow-left" href="/graduation"></a>
	<div class="showImages">
		<ul class="gallery">
			<% graduationImages.forEach(function(image, idx){ %>
			<li><a href="graduation-images/<%= idx %>"><img class="gallery-img" src="<%= image.src %>"/></a></li>	
			<% }); %>
		</ul>
	</div>
</div>